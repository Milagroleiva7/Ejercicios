#include  <iostream>
using namespace std;
int main(){ 
	int hora;
	cout <<"ingrese la hora: "<<"/n";
	cin>>hora;
	if (hora>=0 & hora<12){
		cout<<"Buenos Dias";
	 } else if (hora>=12 & hora<18){
		cout<<"Buenos Tardes";
		 } else if (hora>=18 & hora<24){
		cout<<"Buenos Noches";
	 } else 
	 	cout<<"Hora no valida";
 }
